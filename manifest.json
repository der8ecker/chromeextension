{

    "name": "Darkmode everywhere",
    "description": "This extension shifts every Website to a Darkmode theme. Developed by JoPh Development",
    "version": "2021.04.15",
    "manifest_version": 2,
    "icons": {
        "16": "./logo16.png",
        "32": "./logo32.png",
        "48": "./logo64.png",
        "64": "./logo64.png",
        "128": "./logo128.png",
        "512": "./logo512.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://google.com/*",
        "storage"


    ]
}